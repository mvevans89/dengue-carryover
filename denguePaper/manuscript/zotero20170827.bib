@article{paull2017,
  title = {Drought and Immunity Determine the Intensity of {{West Nile}} Virus Epidemics and Climate Change Impacts},
  volume = {284},
  copyright = {\textcopyright{} 2017 The Author(s). http://royalsocietypublishing.org/licencePublished by the Royal Society. All rights reserved.},
