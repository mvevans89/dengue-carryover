  month = oct,
  year = {2015},
  keywords = {Areal unit problem,Arizona,Public health,Risk maps,West Nile virus},
  pages = {159--169},
  file = {ScienceDirect Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/VIN4MK4I/vonHedemann et al. - 2015 - Visualizations of mosquito risk A political ecolo.pdf:application/pdf;Landscape and Urban Planning 2015 vonHedemann:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/XABAZMCH/Landscape and Urban Planning 2015 vonHedemann.pdf:application/pdf},
