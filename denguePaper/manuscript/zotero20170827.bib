  pages = {558--574},
  file = {PubMed Central Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/ELI4JK8T/Juliano and Lounibos - 2005 - Ecology of invasive mosquitoes effects on residen.pdf:application/pdf},
