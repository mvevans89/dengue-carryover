@article{siraj2017,
  title = {Temperature Modulates Dengue Virus Epidemic Growth Rates through Its Effects on Reproduction Numbers and Generation Intervals},
  volume = {11},
  issn = {1935-2735},
  doi = {10.1371/journal.pntd.0005797},
  abstract = {Author summary Recurrent, rapidly intensifying epidemics of dengue\textendash{}the world's most prevalent mosquito-borne viral disease\textendash{}pose a challenge to healthcare systems throughout the tropical and subtropical world. An acute disease that tends to respond well to proper treatment, the sometimes intense nature of dengue epidemics has been known to overwhelm healthcare systems and elevate the morbidity and mortality of patients left without adequate medical treatment under peak epidemic conditions. Here, we quantify the temperature dependence of dengue epidemic intensity by quantifying two distinct determinants of epidemic growth rate: the average number of secondary infections arising from each primary infection and the average time between successive infections in humans. Our results show that the time between successive infections in humans decreases steadily with increasing temperatures, whereas the average number of secondary infections peaks at intermediate temperatures. Altogether, this suggests a peak temperature for dengue epidemic intensity. Applying this result to global temperature projections under future climate change scenarios suggests that dengue epidemics in many regions of the world could become more intense under future temperature increases.},
  timestamp = {2017-08-02T16:00:38Z},
  number = {7},
  urldate = {2017-08-02},
  journal = {PLOS Neglected Tropical Diseases},
  author = {Siraj, Amir S. and Oidtman, Rachel J. and Huber, John H. and Kraemer, Moritz U. G. and Brady, Oliver J. and Johansson, Michael A. and Perkins, T. Alex},
  month = jul,
