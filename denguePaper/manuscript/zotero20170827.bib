  keywords = {Agriculture,Culex quinquefasciatus,Infectious disease control,Mosquitoes,Oviposition,Photography,Stable isotopes,Texas},
  pages = {e0005347},
  file = {Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/WHACKSNU/Medeiros et al. - 2017 - Dispersal of male and female Culex quinquefasciatu.pdf:application/pdf;Snapshot:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/AHCJCRV8/article.html:text/html}
