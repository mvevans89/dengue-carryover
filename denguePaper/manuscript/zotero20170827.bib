  month = apr,
  year = {2014},
  keywords = {Anopheles albimanus,Climate change,earth sciences,ecology,Gunas,malaria,Plasmodium,social exclusion},
  pages = {e28},
  file = {Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/989Q3UQH/Hurtado et al. - 2014 - When climate change couples social neglect malari.pdf:application/pdf},
