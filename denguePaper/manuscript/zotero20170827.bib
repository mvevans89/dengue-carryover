  pages = {1076--1094},
  file = {JSTOR Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/UMJKRIS9/Zimmerer - 2010 - Retrospective on Nature-Society Geography Tracing.pdf:application/pdf},
