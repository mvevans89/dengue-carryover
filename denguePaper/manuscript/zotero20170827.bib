  file = {Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/GNQQZNNK/Metcalf et al. - 2017 - Identifying climate drivers of infectious disease .pdf:application/pdf},
  pmid = {28814655}
}

@article{dickson2017,
