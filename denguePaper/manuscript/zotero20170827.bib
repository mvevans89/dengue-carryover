  month = jan,
  year = {2004},
  keywords = {BIOTIC communities,CARBON composites,ECOSYSTEM management,FOOD chains (Ecology),population dynamics,PREDATION (Biology)},
  pages = {240--243},
  file = {EBSCO Full Text:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/FKDWRPCA/Pace et al. - 2004 - Whole-lake carbon-13 additions reveal terrestrial .pdf:application/pdf},
