  file = {Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/RW5KVDXC/Kumar et al. - 2016 - Prevalence of Antibodies to Zika Virus in Mothers .pdf:application/pdf}
}

@article{brown2008,
  title = {Remotely-{{Sensed Vegetation Indices Identify Mosquito Clusters}} of {{West Nile Virus Vectors}} in an {{Urban Landscape}} in the {{Northeastern United States}}},
