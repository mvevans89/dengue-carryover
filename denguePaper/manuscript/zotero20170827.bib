  file = {PubMed Central Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/7VD3BVK6/Paaijmans et al. - 2013 - Temperature variation makes ectotherms more sensit.pdf:application/pdf},
  pmid = {23630036},
  pmcid = {PMC3908367}
}

@article{parton1981,
