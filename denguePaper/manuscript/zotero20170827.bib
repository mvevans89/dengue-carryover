  month = jan,
  year = {2003},
  keywords = {aquatic hyphomycetes,bacteria,conidial production,fungal biomass,respiration},
  pages = {123--134},
  file = {Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/XFMHBZ38/Gulis and Suberkropp - 2003 - Leaf litter decomposition and microbial activity i.pdf:application/pdf;Snapshot:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/QQF5DBNB/abstract.html:text/html}
