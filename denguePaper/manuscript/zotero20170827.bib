  file = {SAGE PDF Full Text:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/K53WEWHC/Chaplin - 2011 - Indian cities, sanitation and the state the polit.pdf:application/pdf}
}

@article{blagrove2017,
  title = {Co-Occurrence of Viruses and Mosquitoes at the Vectors' Optimal Climate Range: {{An}} Underestimated Risk to Temperate Regions?},
  volume = {11},
  issn = {1935-2735},
  shorttitle = {Co-Occurrence of Viruses and Mosquitoes at the Vectors' Optimal Climate Range},
  doi = {10.1371/journal.pntd.0005604},
  abstract = {Author summary Mosquito-borne viruses, such as dengue, are believed to cause over 100 million cases of human disease annually. Current mathematical models that aim to predict risk of virus transmission are generally either highly ``mosquito-centric'' or ``virus-centric''. For virus transmission to occur, conditions need to be suitable for both mosquito and virus: hence, we propose a novel approach that considers the interplay between the different optimal conditions for the mosquito and the virus. Our findings indicate that warmer- or colder- adapted mosquitoes are significantly more efficient vectors in warmer or colder climates respectively. Consequently, we propose that there is currently an underestimation of risk to temperate regions from their native and cooler adapted mosquitoes.},
  timestamp = {2017-07-13T18:03:02Z},
  number = {6},
  urldate = {2017-07-13},
