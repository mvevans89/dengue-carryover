  month = jul,
  year = {2012},
  keywords = {cholera and flooding,disease forecasting,inhomogeneous Markov chain,risk propagation,spatiotemporal dynamics of urban cholera},
  pages = {2033--2036},
  file = {Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/IRF4F3SN/Reiner et al. - 2012 - Highly localized sensitivity to climate forcing dr.pdf:application/pdf},
  pmid = {22308325}
