  month = aug,
  year = {2015},
  pages = {20140303},
  file = {Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/9TCHQQE4/Sofonea et al. - 2015 - From within-host interactions to epidemiological c.pdf:application/pdf;Snapshot:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/2Q8C6E62/20140303.html:text/html},
  pmid = {26150669}
