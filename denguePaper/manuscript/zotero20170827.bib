  author = {Prado, G. P. and Maciel, J. S. and Leite, G. R. and Souza, M. a. A.},
  month = jun,
  year = {2017},
  keywords = {abiotic factors,Aedes aegypti,Brazil,Chikungunya virus,deforestation,environment,kernel,Oviparity,ovitrap,Stegomyia,vector},
  pages = {155--160},
  file = {EBSCO Full Text:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/GI739522/Prado et al. - 2017 - Influence of shading and pedestrian traffic on the.pdf:application/pdf},
