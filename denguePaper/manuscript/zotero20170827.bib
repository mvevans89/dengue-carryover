  month = nov,
  year = {2004},
  keywords = {Chile,export agriculture,Norte Chico,Political ecology,social power,water markets,water rights},
  pages = {322--342},
  file = {Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/RA88N98T/Budds - 2004 - Power, Nature and Neoliberalism The Political Eco.pdf:application/pdf;Snapshot:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/FD9XCNQT/abstract.html:text/html},
  groups = {Pol Ecol of Disease}
