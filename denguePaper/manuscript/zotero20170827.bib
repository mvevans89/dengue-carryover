  month = jun,
  year = {2017},
  keywords = {Aedes aegypti,Chikungunya virus,Dengue virus,Mosquitoes,Sequence databases,Temperate regions,Viral vectors,West Nile virus},
  pages = {e0005604},
  file = {Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/IGIVRPW8/Blagrove et al. - 2017 - Co-occurrence of viruses and mosquitoes at the vec.pdf:application/pdf}
}

