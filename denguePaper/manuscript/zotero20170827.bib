  month = mar,
  year = {2016},
  keywords = {Antibodies,Brazil,Chikungunya infection,Infectious disease epidemiology,Microcephaly,Public and occupational health,Zika fever,Zika virus},
  pages = {e0004530},
  file = {Full Text PDF:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/GQ4NHURB/Malone et al. - 2016 - Zika Virus Medical Countermeasure Development Cha.pdf:application/pdf}
