@article{lambrechts2011,
  title = {Impact of Daily Temperature Fluctuations on Dengue Virus Transmission by {{Aedes}} Aegypti},
  volume = {108},
  doi = {10.1073/pnas.1101377108},
  abstract = {Most studies on the ability of insect populations to transmit pathogens consider only constant temperatures and do not account for realistic daily temperature fluctuations that can impact vector-pathogen interactions. Here, we show that diurnal temperature range (DTR) affects two important parameters underlying dengue virus (DENV) transmission by Aedes aegypti. In two independent experiments using different DENV serotypes, mosquitoes were less susceptible to virus infection and died faster under larger DTR around the same mean temperature. Large DTR (20 $^\circ$C) decreased the probability of midgut infection, but not duration of the virus extrinsic incubation period (EIP), compared with moderate DTR (10 $^\circ$C) or constant temperature. A thermodynamic model predicted that at mean temperatures $<$18 $^\circ$C, DENV transmission increases as DTR increases, whereas at mean temperatures $>$18 $^\circ$C, larger DTR reduces DENV transmission. The negative impact of DTR on Ae. aegypti survival indicates that large temperature fluctuations will reduce the probability of vector survival through EIP and expectation of infectious life. Seasonal variation in the amplitude of daily temperature fluctuations helps to explain seasonal forcing of DENV transmission at locations where average temperature does not vary seasonally and mosquito abundance is not associated with dengue incidence. Mosquitoes lived longer and were more likely to become infected under moderate temperature fluctuations, which is typical of the high DENV transmission season than under large temperature fluctuations, which is typical of the low DENV transmission season. Our findings reveal the importance of considering short-term temperature variations when studying DENV transmission dynamics.},
  timestamp = {2016-11-28T20:50:32Z},
  number = {18},
  journal = {PNAS},
  author = {Lambrechts, L and Paaijmans, K P and Fansiri, T and Carrington, L B and Kramer, L D and Thomas, M B and Scott, T W},
  month = may,
  year = {2011},
  pages = {7460--7465},
  file = {3A888E9E-6A49-49D2-AFE8-63BB29E797BE:/Users/mvevans/Library/Application Support/Zotero/Profiles/7gwsnntd.default/zotero/storage/EQCXVJCZ/3A888E9E-6A49-49D2-AFE8-63BB29E797BE.pdf:application/pdf},
  groups = {papersRIS}
}

